<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.ProfileMenuController"
            styleClass="BackGroundColor"
        prefHeight="600.0" prefWidth="500.0">
    <!--            stylesheets="@../CSS/DefaultStyle.css"-->
    <!--            styleClass="BackGround"-->

    <center>
        <VBox alignment="CENTER" spacing="15">
            <PasswordField promptText="New Password" fx:id="newPassword" maxWidth="300"/>
            <Button text="Submit" onMouseClicked="#setNewPassword" maxWidth="150"/>
            <Button text="Back" onMouseClicked="#enterProfileMenu" maxWidth="150"/>
        </VBox>
    </center>
</BorderPane>
